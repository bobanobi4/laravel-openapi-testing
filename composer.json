{
    "name": "vfunin/laravel-openapi-testing",
    "description": "",
    "keywords": [
        "laravel",
        "factory"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Leonid Gorshkov",
            "email": "synacs.user@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "vfunin/openapi-httpfoundation-testing": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "mockery/mockery": "1.7.x-dev",
        "orchestra/testbench": "8.x-dev",
        "pestphp/pest": "^2.23",
        "php-parallel-lint/php-var-dump-check": "dev-master",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Mib\\LaravelOpenApiTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mib\\LaravelOpenApiTesting\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "cs": "php-cs-fixer fix --config .php-cs-fixer.php",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
